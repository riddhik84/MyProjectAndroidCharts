<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.riddhik.myapps.myprojectandroidcharts">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MPAndroidChartActivity"
            android:label="@string/title_activity_mpandroid_chart"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.riddhik.myapps.myprojectandroidcharts.MainActivity" />
        </activity>
        <activity
            android:name=".MPAndroidLineChartActivity"
            android:label="@string/title_activity_mpandroid_line_chart"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.riddhik.myapps.myprojectandroidcharts.MainActivity" />
        </activity>
        <activity
            android:name=".SampleActivity"
            android:label="@string/title_activity_sample"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.riddhik.myapps.myprojectandroidcharts.MainActivity" />
        </activity>
    </application>

</manifest>